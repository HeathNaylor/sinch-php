{
  "name": "heathnaylor/sinch-php",
  "type": "wrapper",
  "description": "PHP Wrapper for Sinch API",
  "keywords": [
    "sinch"
  ],
  "homepage": "https://github.com/HeathNaylor/sinch-php",
  "authors": [
    {
      "name": "Heath Naylor",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "HeathNaylor\\SinchPhp\\": "Src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HeathNaylor\\Tests\\": "Tests/"
    },
    "classmap": [
      "Tests"
    ]
  },
  "require": {
    "guzzlehttp/guzzle": "^6.2",
    "league/route": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.6.2"
  }
}
